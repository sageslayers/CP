#include <bits/stdc++.h>
#include <string.h>

using namespace std;
int main() {
	
	int a; 
	int div1,div2;
	int b;
	int c;
	int i ; 
	int count = 0 ; 
	char print[10001][10];
	while(scanf("%i",&a), a) {
		
		scanf("%i %i",&div1,&div2);
		for ( i = 0 ; i < a  ; i ++ ) {
			scanf("%i %i",&b,&c);
			if( b == div1 || c == div2 ){
			strcpy(print[count],"divisa" );
			
			}
			else if ( b > div1 && c > div2 ) {
				strcpy(print[count],"NE" ); 
			} 
			else if ( b < div1 && c>div2 ) {
				strcpy(print[count],"NO" );
			}
			else if ( b < div1 && c<div2 ) {
				strcpy(print[count],"SO" );
			}
			else if (b > div1 && c < div2 ) {
				strcpy(print[count],"SE" );
			}
			count ++;
		}
		
	}
	for ( i = 0 ; i  < count ; i++ ) {
		printf("%s\n",print[i]) ;	
	}
	
}
